{
    "name": "freepeace13/laravel-eloquent-category",
    "authors": [
        {
            "name": "Kin Basco",
            "email": "freepeace13@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "kalnoy/nestedset": "^5.0",
        "ankurk91/laravel-eloquent-relationships": "^1.1",
        "illuminate/auth": "^7.29",
        "illuminate/container": "^7.29",
        "illuminate/contracts": "^7.29",
        "illuminate/database": "^7.29"
    },
    "require-dev": {
        "orchestra/testbench": "^5.9",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "EloquentCategory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EloquentCategory\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EloquentCategory\\EloquentCategoryServiceProvider"
            ]
        }
    }
}
